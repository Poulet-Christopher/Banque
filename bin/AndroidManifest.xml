<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.banque"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="19"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.banque.MainActivity"
            android:label="@string/title_activity_main" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.banque.Ajouter"
            android:label="@string/title_activity_ajouter" >
        </activity>
        <activity
            android:name="com.example.banque.Editer"
            android:label="@string/title_activity_editer" >
        </activity>
        <activity
            android:name="com.example.banque.Historique"
            android:label="@string/title_activity_historique" >
        </activity>
    </application>

</manifest>
